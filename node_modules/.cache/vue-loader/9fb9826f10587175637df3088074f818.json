<div class=\"easyFm\">\n    <div class=\"easyFm-header\">\n      <div class=\"easyFm-header-log\">this is log</div>\n      <div class=\"easyFm-header-list\">\n        <el-menu :default-active=\"mainListStatus\" class=\"el-menu-demo\" mode=\"horizontal\" router>\n          <el-menu-item v-for=\"i in mainList\" :key=\"i.id\" :index=\"i.path\" >{{i.name}}</el-menu-item>\n        </el-menu>\n      </div>\n    </div>\n    <div class=\"easyFm-main\">\n      <div class=\"easyFm-main-list\">\n        <el-button @click=\"isCollapse = !isCollapse\">1</el-button>\n        <el-menu default-active=\"1-4-1\" class=\"el-menu-vertical-demo\" :collapse=\"isCollapse\" router>\n          <template v-for=\"i in leftList\">\n            <el-submenu v-if=\"i.children.length > 0\" :index=\"i.path\" :key=\"i.id\" >\n              <template slot=\"title\">\n                <i class=\"el-icon-location\"></i>\n                <span slot=\"title\">{{i.name}}</span>\n              </template>\n              <el-menu-item v-for=\"ii in i.children\" :key=\"ii.id\" :index=\"ii.path\">{{ii.name}}</el-menu-item>\n            </el-submenu>\n            <el-menu-item v-else :index=\"i.path\" :key=\"i.id\">\n              <i class=\"el-icon-menu\"></i>\n              <span slot=\"title\">{{i.name}}</span>\n            </el-menu-item>\n          </template>\n        </el-menu>\n      </div>\n      <div class=\"easyFm-main-content\">\n        <el-breadcrumb separator=\"/\">\n          <el-breadcrumb-item :to=\"{ path: '/' }\">首页</el-breadcrumb-item>\n          <el-breadcrumb-item :to=\"{ path: '/about' }\" >活动管理</el-breadcrumb-item>\n          <el-breadcrumb-item><a href=\"/About\">活动列表</a></el-breadcrumb-item>\n          <el-breadcrumb-item>活动详情</el-breadcrumb-item>\n        </el-breadcrumb>\n        <router-view/>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { mapState, mapActions } from 'vuex'\nexport default {\n  data() {\n    return {\n      isCollapse: false,\n      mainList: [],\n      leftList: [],\n      mainListStatus: '/'\n    }\n  },\n  computed: {\n    ...mapState(['updateUserInfo'])\n  },\n  methods: {\n    handleSelect(index) {\n        console.log(index)\n    }\n  },\n  mounted() {\n    this.mainListStatus = this.$route.path\n    this.mainList = this.updateUserInfo.app_menu\n    for (const key in this.mainList) {\n      \n      if (this.mainList[key].path == this.mainListStatus) {\n        console.log(this.mainList[key].children)\n        this.leftList = this.mainList[key].children\n      }\n    }\n  }\n}\n</script>\n\n<style lang=\"less\" scoped>\n.el-menu-vertical-demo:not(.el-menu--collapse) {\n  width: 200px;\n  min-height: 400px;\n}\n.easyFm {\n  height: 100%;\n  .easyFm-header {\n    width: 100%;\n    .easyFm-header-log, .easyFm-header-list {\n      height: 60px;\n      float: left\n    }\n    .easyFm-header-log {\n      background: #fff;\n      text-align: center;\n      line-height: 60px;\n      width: 300px;\n    }\n    .easyFm-header-list {\n      width: calc(100% - 300px)\n    }\n    &:after {\n      content: '';\n      display: block;\n      clear: both;\n    }\n  }\n  .easyFm-main {\n    width: 100%;\n    height: 100%;\n    .easyFm-main-list, .easyFm-main-content {\n      height: 100%;\n      float: left;\n    }\n    .easyFm-main-list {\n      background-color: #fff;\n    }\n    .easyFm-main-content {\n      width: calc(100% - 300px)\n    }\n  }\n}\n</style>\n\n"]}]}